<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="iBusiness" parallel="classes" thread-count="5" preserve-order="true">
	<listeners>
		<listener class-name="testnglisteners.ExtentTestNGITestListener"></listener>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>
	<test name="iBusiness">
		<classes>
			<class name="iBusinessAPI.CodingAssignment" >
			<methods>
			<include name="codeAssignment"/>
			</methods>
			</class>
		</classes>
	</test>
</suite>